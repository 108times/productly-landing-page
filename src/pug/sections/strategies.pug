- var data = strategies
section.site-section.strategies
    .container.strategies__container
        .strategies__description
            h1.strategies__title #{data.title}
            p.strategies__text #{data.text}
        .strategies__content
            .swiper.strategies__slider
                .swiper-wrapper
                    each val,key in data.items
                        +article('strategies__slide swiper-slide col-md-4 col-sm-12', val)
                .swiper-pagination.strategies__pagination

